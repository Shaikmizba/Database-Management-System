CREATE TABLE STUDENT (
  S_NO INT(2),
  Reg_No VARCHAR(100),
  NAME VARCHAR(100),
  GENDER CHAR(1),
  MOBILE_No VARCHAR(10),
  CITY VARCHAR(20)
);  
