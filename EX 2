CREATE TABLE FACULTY (
  FAC_NO VARCHAR(100),
  FAC_NAME VARCHAR(100),
  GENDER CHAR(1),
  MOBILE_NO VARCHAR(10),
  CITY VARCHAR(30)
  );  
